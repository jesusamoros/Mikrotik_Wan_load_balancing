name="check_WANS_final" owner="admin" policy=ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon dont-require-permissions=no last-started=nov/16/2018 09:08:20 run-count=8085 
     source=
       :if ([/interface get [/interface find name="ether8-WAN8"] running] = true) do={
           :if ([/ping 208.67.222.222 count=5 interface=ether8-WAN8] = 0) do={
           /interface disable ether8-WAN8
           /tool e-mail send to="soporte@mail.com" cc="noc@mail.com"  subject="wan caida" body="Hola, este es un mensaje de alerta del Mikrotik1, revisar el router conectado a la interfaz ether8-WAN8  WAN  CAIDA  \nDONDE:SITE>
           /tool e-mail send to="youremail@mail.com" cc="yourmail@mail.com" subject="wan caida" body="interfaz ether8-WAN8  WAN mail.comstart-tls=yes
           }
           }
       
       
       
